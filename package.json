{
  "name": "f5-automation-config-converter",
  "version": "1.23.0",
  "author": "F5 Networks",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/f5devcentral/f5-automation-config-converter.git"
  },
  "description": "F5 Automation Config Converter",
  "scripts": {
    "test": "NODE_ENV=test mocha test --recursive --timeout 60000 --exit",
    "watch": "NODE_ENV=test mocha test --recursive --watch --timeout 30000 --exit",
    "lint": "eslint .",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc npm test",
    "start": "node init.js serve",
    "copyright": "node scripts/dev/write-copyright.js cicd",
    "build": "source scripts/build/image-build.sh",
    "test-mutation": "stryker run"
  },
  "dependencies": {
    "@automation-toolchain/f5-appsvcs-schema": "file:dependencies/automation-toolchain-f5-appsvcs-schema-0.4.16.tgz",
    "@f5devcentral/f5-teem": "^1.5.0",
    "commander": "^9.4.0",
    "decompress": "^4.2.1",
    "deepmerge": "^4.2.2",
    "express": "^4.18.1",
    "jiff": "^0.7.3",
    "multer": "^1.4.5-lts.1",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@f5devcentral/atg-shared-utilities": "^0.4.6",
    "@f5devcentral/eslint-config-f5-atg": "latest",
    "@stryker-mutator/core": "^6.1.2",
    "@stryker-mutator/mocha-runner": "^6.1.2",
    "ajv": "6.12.6",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jsonpointer": "^5.0.1",
    "mocha": "^10.0.0",
    "node-forge": "^1.3.1",
    "nyc": "^15.1.0",
    "sinon": "^14.0.0"
  },
  "eslintConfig": {
    "extends": [
      "@f5devcentral/eslint-config-f5-atg"
    ],
    "parserOptions": {
      "ecmaVersion": 2021
    }
  },
  "eslintIgnore": [
    "autotoolDeps",
    "scripts"
  ],
  "nyc": {
    "reporter": [
      "html",
      "text",
      "json-summary"
    ],
    "include": "src"
  },
  "bugs": {
    "url": "https://github.com/f5devcentral/f5-automation-config-converter/issues"
  },
  "homepage": "https://github.com/f5devcentral/f5-automation-config-converter#readme",
  "main": "init.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  }
}
